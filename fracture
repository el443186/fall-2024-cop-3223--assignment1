double calculateDistance()
{
  double d1=0;
  double d2=0;
  double distance=0;
  double x1,x2,y1,y2
  printf ("What is X1?");
  scanf("%lf", &x1);
  printf("X1",&x1); 

  printf ("What is Y1?");
  scanf("%lf", &y1);
  printf("Y1",&y1); 

  printf ("What is X2?");
  scanf("%lf", &x2);
  printf("X2",&x2); 

  printf ("What is Y2?");
  scanf("%lf", &y2);
  printf("Y2",&y2); 

  d1=x2-x1;
  d2=y1-y2;
  distance=sqrt((d1*d1)+(d2*d2));
return distance;

